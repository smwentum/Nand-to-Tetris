/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=a, out=a1);
    Not(in=b, out=b1);

    And(a=a,b=b1,out=a2);
    And(a=a1,b=b,out=b2);

    Or(a=a2,b=b2,out=out);
}
/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    //// I am going to guess nand(not(a),not(b)) maybye there is something simpler but i am not sure
    Not(in=a,out=a1);
    Not(in=b,out=b1);
    Nand(a=a1,b=b1,out=out);
}
